<div class="list-block media-list">
  <ul>
    <% @results.each do |c| %>
      <li>
    <a href="/clinic?id=<%= c.id %>" class="item-link item-content">
      <div class="item-media">
      </div>
      <div class="item-inner">
        <div class="item-title-row">
          <div class="item-title"><%= c.name %></div>
          <div class="item-after"><%= c.station %></div>
        </div>
        <div class="item-subtitle"><%= c.canon %></div>
        <div class="item-text"><%= c.address %><br><%= c.tel %></div>
      </div>
    </a>
    </li>
  <% end %>
  </ul>
</div>
